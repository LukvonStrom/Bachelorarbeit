@inproceedings{A.I.Stojnev.2017,
 author = {{A. I. Stojnev} and {D. H. Stojanovi{\'c}}},
 title = {{Software systems for processing and analysis of big data and event streams}},
 keywords = {Big Data;big data analysis;big data processing;contemporary stream processing frameworks;data collection;data flow computing;data mining;data streams processing;data visualisation;data visualization;decision support;decision support systems;event streams;Flink;index structures;Internet of Things;IoT;mobile positioning;Public transportation;Real-time systems;resource allocation;sensor technologies;software systems;Spark;Sparks;Storm;Storms;Stream processing and analysis;Topology;Twitter;wireless communications},
 pages = {128--131},
 bookpagination = {page},
 booktitle = {{2017 13th International Conference on Advanced Technologies, Systems and Services in Telecommunications (TELSIKS)}},
 year = {2017},
 abstract = {Advances in sensor technologies, mobile positioning, IoT and wireless communications have led to development of applications for data collection, processing, analysis, mining and visualization for the purposes of real time responses and decision support, but have also brought new challenges to processing large amount of data in a real time. Various software systems are developed to provide a set of methods, technologies, algorithms and index structures for effective monitoring, processing, analysis and mining of continuous flow of data and events. This paper reviews recent developments in the field of data streams processing and analysis, gives an overview of existing systems and technologies, and demonstrates development of a stream processing application based on the contemporary stream processing frameworks.},
 doi = {10.1109/TELSKS.2017.8246245},
 eventtitle = {2017 13th International Conference on Advanced Technologies, Systems and Services in Telecommunications (TELSIKS)}
}


